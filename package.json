        "update:minor": "ncu -u -t minor && npm update",
        "update:major": "ncu -u && npm update"
    },
    "dependencies": {
        "@nextui-org/button": "^2.2.7",
        "@nextui-org/card": "^2.2.7",
        "lucide-react": "^0.468.0"
