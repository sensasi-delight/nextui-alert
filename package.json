        "@rollup/plugin-typescript": "^12.1.1",
        "@types/eslint__js": "^8.42.3",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/react": "^18.3.16",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "npm-check-updates": "^17.1.11",
